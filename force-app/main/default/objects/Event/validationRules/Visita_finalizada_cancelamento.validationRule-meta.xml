<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Visita_finalizada_cancelamento</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISCHANGED( Fim_da_visita__c  )
,
Fim_da_visita__c  = false)</errorConditionFormula>
    <errorDisplayField>Fim_da_visita__c</errorDisplayField>
    <errorMessage>Você não pode reiniciar uma visita finalizada.</errorMessage>
</ValidationRule>
