<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Converter_Michelin</fullName>
    <active>true</active>
    <errorConditionFormula>AND( 
RecordType.DeveloperName = &#39;Michelin&#39;, 
IsConverted = true, 
ISBLANK(TRIM(CNPJ_CPF__c))
)</errorConditionFormula>
    <errorMessage>Para prosseguir com a conversão do lead é necessário informar um CPF ou CNPJ válido.</errorMessage>
</ValidationRule>
