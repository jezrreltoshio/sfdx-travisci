<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Mercosul_invalido_DNI</fullName>
    <active>true</active>
    <description>Não permite alguns DNIs invalidos</description>
    <errorConditionFormula>OR(
 DNI__c = &#39;00.000.000&#39;,
 DNI__c = &#39;11.111.111&#39;,
 DNI__c = &#39;22.222.222&#39;,
 DNI__c = &#39;33.333.333&#39;,
 DNI__c = &#39;44.444.444&#39;,
 DNI__c = &#39;55.555.555&#39;,
 DNI__c = &#39;66.666.666&#39;,
 DNI__c = &#39;77.777.777&#39;,
 DNI__c = &#39;88.888.888&#39;,
 DNI__c = &#39;99.999.999&#39;,
 DNI__c = &#39;12.345.678&#39;
)</errorConditionFormula>
    <errorDisplayField>DNI__c</errorDisplayField>
    <errorMessage>DNI inválido</errorMessage>
</ValidationRule>
