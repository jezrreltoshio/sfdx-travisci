<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Se_cliente_sascar_CPF_obrigatorio</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
 Cliente_Sascar__c = true,
 ISBLANK( CNPJ_CPF__c )
)</errorConditionFormula>
    <errorDisplayField>CNPJ_CPF__c</errorDisplayField>
    <errorMessage>Para cliente Sascar, o CNPJ/CPF é obrigatório.</errorMessage>
</ValidationRule>
