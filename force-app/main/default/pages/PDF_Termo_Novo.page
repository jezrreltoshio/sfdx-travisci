          <br></br>
          <br></br>Data:_______________________________________________
       </apex:column>
      </apex:DataTable>
      
      <apex:dataTable columns="2" value="{!Opportunity}" var="opp" border="1" style="border-collapse:collapse; page-break-before: always;" width="100%">
