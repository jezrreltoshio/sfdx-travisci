        <apex:column colspan="1" width="20%" styleClass="fieldLabel" breakBefore="true" style="size: 8px; background-color:#DBE5E6;">Telefone2</apex:column>
          <apex:column colspan="1" width="30%" styleClass="fieldData" style="size: 8px;" value="{!SOS_Contact1_Tel2}" ></apex:column>
        <apex:column colspan="1" width="20%" styleClass="fieldLabel" style="size: 8px; background-color:#DBE5E6;">Telefone4</apex:column>
