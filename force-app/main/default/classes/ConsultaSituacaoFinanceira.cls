    public ConsultaSituacaoFinanceira(){
        Id accId = ApexPages.currentPage().getParameters().get('id');
        idAcc = accId;
        
        conta = [Select Id, 
                    Conta__c,
                    Faturamento_mensal__c,
                    Faturamento_medio_trimestral__c,
                    Ticket_medio_recorrente__c,
                    Situacao_financeira__c,
                    Divida__c,
                    Dias_em_atraso_divida__c,
                    Data_do_ultimo_pagamento__c,
                    Valor_ultimo_pagamento__c,
                    Situacao_sinal__c,
                    Tempo_medio_de_atraso__c,
                    Experiencia_do_cliente__c,
                    Maior_atraso__c,
                    Data_do_proximo_pagamento__c,
                    Valor_proximo_pagamento__c,
                    Forma_de_pagamento__c,
                    Dia_de_vencimento_do_pagamento__c,
                    Acao_judicial__c,
                    Rentabilidade_Mensal__c,
                    Rentabilidade_YTD__c,
                    Termos_ativos__c,
                    Pendente_reinstalacao__c,
                    Pre_rescisao__c,
                    Recisao_por_inadimplencia__c,
                    Recisao_total__c,
                    Stop_de_faturamento__c,
                    Roubado_e_nao_recuperado__c,
                    Roubado_e_recuperado__c                                                        
                From Situacao_Financeira__c
                WHERE Conta__c=:accId
                ORDER BY CreatedDate DESC LIMIT 1
        ];
        
        if(conta.size() > 0){
            show = true;
        } else{
            show = false;
        }
    }
    
